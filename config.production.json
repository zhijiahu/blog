{
    "url": process.env.HEROKU_URL,
    "mail": {
        "transport": 'SMTP',
        "options": {
            "service": 'Mailgun',
            "auth": {
                "user": process.env.MAILGUN_SMTP_LOGIN,
                "pass": process.env.MAILGUN_SMTP_PASSWORD
            }
        }
    },
    "database": {
        "client": 'mysql',
        "connection": process.env.JAWSDB_URL,
        "debug": false
    }
}
